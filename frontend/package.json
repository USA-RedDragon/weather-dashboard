{
  "name": "weather-dashboard",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:e2e": "start-server-and-test preview 127.0.0.1:4173 'cypress run --e2e --headed'",
    "test:e2e:chrome": "start-server-and-test preview 127.0.0.1:4173 'cypress run --e2e --headed --browser chrome'",
    "test:e2e:firefox": "start-server-and-test preview 127.0.0.1:4173 'cypress run --e2e --headed --browser firefox'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' 127.0.0.1:4173 'cypress open --e2e'",
    "test:unit": "vitest run",
    "test:unit:dev": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "lint:fix": "npm run lint -- --fix",
    "lint:report": "npm run lint -- --output-file eslint_report.json --format json"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "d3": "^7.9.0",
    "d3-geo": "^3.1.1",
    "d3-geo-projection": "^4.0.0",
    "great-circle-cursor": "^1.0.0",
    "pinia": "^2.3.1",
    "pixi.js": "^7.4.3",
    "primeflex": "^3.3.1",
    "primeicons": "^7.0.0",
    "primevue": "^4.4.1",
    "vue": "^3.5.22",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@msgpack/msgpack": "^3.1.2",
    "@rushstack/eslint-patch": "^1.14.0",
    "@tsconfig/node20": "^20.1.6",
    "@types/d3": "^7.4.3",
    "@types/d3-geo": "^3.1.0",
    "@types/d3-selection": "^3.0.11",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.19.23",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitest/ui": "^2.1.9",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "cypress": "^13.17.0",
    "cypress-mochawesome-reporter": "^3.8.4",
    "cypress-multi-reporters": "^1.6.4",
    "eslint": "^8.57.1",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-cypress": "^3.6.0",
    "eslint-plugin-vue": "^10.5.1",
    "isomorphic-fetch": "^3.0.0",
    "jsdom": "^25.0.1",
    "mocha-junit-reporter": "^2.2.1",
    "mochawesome": "^7.1.4",
    "mochawesome-merge": "^4.4.1",
    "mochawesome-report-generator": "^6.3.0",
    "npm-run-all2": "^6.2.6",
    "prettier": "^3.6.2",
    "sass": "^1.93.2",
    "start-server-and-test": "^2.1.2",
    "typescript": "~5.9.3",
    "vite": "^5.4.21",
    "vite-plugin-mkcert": "^1.17.9",
    "vite-plugin-vue-devtools": "^7.7.7",
    "vitest": "^2.1.9",
    "vue-tsc": "^2.2.12"
  }
}
